type Order {
    id: String!
    Price: Float!
    Tax: Float!
    FinalPrice: Float!
}

input OrderInput {
    id : String!
    Price: Float!
    Tax: Float!
}

type GetAllOrdersOutput {
    orders: [Order]
}

type Mutation {
    createOrder(input: OrderInput): Order
}

type Query {
    getAllOrders: GetAllOrdersOutput
}